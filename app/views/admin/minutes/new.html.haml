%h1= model.new_record? ? t('new_minutes') : t('edit_minutes')
- form_for [:admin, model], :url => (model.new_record? ? admin_minutes_url() : admin_minutes_url(model)) do |f|
  = render 'fields', :object => f

= render 'popups'
= render 'admin/minutes/edit_scripts'

- content_for 'page_scripts', minutes_edit_javascripts
